{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.itcActivitybook",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.itcActivitybook.view.app",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.itcActivitybook.i18n.i18n"
				}
			},
			"Data": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/Data.json"
			},
			
				"jsonData":{
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/newdata.json"
			},
			
				"json":{
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/tabledata.json"
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.itcActivitybook.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			},

			"routes": [{
				"pattern": "",
				"name": "appHome",
				"target": "home"
			}, {
				"pattern": "activities/:row:",
				"name": "activies",
				"target": "activies"
			}, {
				"pattern": "activityDetail",
				"name": "activityDetail",
				"target": "activityDetail"
			}, {
				"pattern": "reports",
				"name": "reports",
				"target": "reports"
			},
			{
				"pattern": "activityBook",
				"name": "booklist",
				"target": "booklist"
			}
			
			
			
			
			
			
			],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "launchpad",
					"viewLevel": 1

				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				},
				"activies": {

					"viewId": "dailyactivity",

					"viewName": "Dailyactivity",
					"viewLevel": 2

				},
				"activityDetail": {
					"viewId": "employee",
					"viewName": "Activitydetails",
					"viewLevel": 2
				},

				"reports": {
					"viewId": "reports",
					"viewName": "reports",
					"viewLevel": 2
				
				},
					"booklist": {
					"viewId": "activitylist",
					"viewName": "activitybooklist",
					"viewLevel": 2
				
				}
				
				
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}