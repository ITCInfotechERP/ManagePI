<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="handleConfirm">
		<sortItems>
			<ViewSettingsItem text="Discipline ID" key="DisciplineID" selected="true" />
			<ViewSettingsItem text="Group ID" key="GroupID" />
		    <ViewSettingsItem text="Code ID" key="CodeID" />
			
		</sortItems>
		<groupItems>
			<ViewSettingsItem text="Discipline ID" key="DisciplineID"/>
			<ViewSettingsItem text="Group ID" key="GroupID" />
		</groupItems>
		
		<filterItems>
			
			<ViewSettingsFilterItem
				text="Hours"
				key="Hours"
				multiSelect="false">
				<items>
					<ViewSettingsItem text="Less Than 5" key="Hours___LE___5___X" />
					<ViewSettingsItem text="Between 5 and 8" key="Hours___BT___5___8" />
					<ViewSettingsItem text="More Than 10" key="Price___GT___10___X" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>